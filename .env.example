# PostgreSQL Database
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=taskqueue

# Connection URLs (for local development without Docker)
# FastAPI uses asyncpg driver
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/taskqueue

# Hono/Bun uses postgres.js driver
DATABASE_URL=postgres://postgres:postgres@localhost:5432/taskqueue
